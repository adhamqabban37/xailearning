{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=9.0.0"
  },
  "packageManager": "npm@10.9.2",
  "scripts": {
    "dev": "next dev -p 9002",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "start": "next start",
    "start:prod": "next start -p 9010",
    "smoke": "cross-env BASE_URL=http://localhost:9010 node ./scripts/smoke.mjs",
    "repair:resources": "node ./scripts/repair-resources.mjs",
    "validate:youtube": "node ./scripts/validate-all-youtube.mjs",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "e2e:headless": "cypress run --headless",
    "clean": "rm -rf .next out node_modules/.cache",
    "postinstall": "patch-package",
    "ci:unit": "npm run typecheck && npm run lint && npm test",
    "ci:e2e": "start-server-and-test start:prod http://localhost:9010 e2e",
    "ci": "npm run ci:unit && npm run ci:e2e",
    "prepare": "node -e \"try { require('husky') } catch (e) {}\"",
    "audit:fix": "npm audit fix",
    "update:check": "npm outdated"
  },
  "dependencies": {
    "@genkit-ai/google-genai": "^1.20.0",
    "@genkit-ai/next": "^1.20.0",
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@react-pdf/renderer": "^4.3.1",
    "@supabase/supabase-js": "^2.76.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "embla-carousel-react": "^8.6.1",
    "firebase": "^12.4.0",
    "framer-motion": "^11.18.3",
    "genkit": "^1.20.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.475.0",
    "multer": "^1.4.5-lts.1",
    "next": "15.3.3",
    "next-connect": "^1.0.0",
    "node-fetch": "^3.3.2",
    "patch-package": "^8.0.0",
    "pdf-parse": "^1.1.1",
    "react": "^19.0.0",
    "react-day-picker": "^9.4.4",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-pdf": "^10.2.0",
    "react-resizable-panels": "^2.1.7",
    "recharts": "^2.15.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/cypress": "^10.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.13",
    "@types/node": "^22.10.5",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.2",
    "cypress": "^13.17.0",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.3.3",
    "genkit-cli": "^1.20.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.4.18",
    "ts-jest": "^29.4.5",
    "typescript": "^5.7.2"
  },
  "overrides": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
